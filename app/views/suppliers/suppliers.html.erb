<h2>您的包裹</h2>

  <% @package_ids.each do |package_id| %>    <% package = Package.find(package_id) %>
    <div class="a">包裹名称</div>
    <%= package.name %>
    <% package.orders.each do |order| %>
      <ul>商品名称</ul> <%= order.commodity.name %> <span>数量</span> <%= order.amount %> <span>总价</span> <%= order.order_price %>
    <% end %>
  <% end %>
 


